{
  "name": "docker_guild_session_tracker",
  "packageManager": "yarn@3.2.3",
  "workspaces": [
    "packages/*"
  ],
  "dependencies": {
    "typescript": "^4.8.2"
  },
  "scripts": {
    "start": "docker-compose -f docker-compose.yml -f ./packages/api/docker-compose.api.yml -f ./packages/database/docker-compose.database.yml -f ./packages/website/docker-compose.website.yml up",
    "stop": "docker-compose down --remove-orphans",
    "prune-db": "docker volume rm dgsession_db_data"
  }
}
